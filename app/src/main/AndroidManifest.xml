<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.lexshop">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".home.HomeActivity"
                android:label="@string/title_activity_home"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".home.Load"
                android:label="@string/title_activity_load"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".Team"
                android:label="@string/title_activity_team"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".admin.AdminCategoryActivity">
        </activity>
        <activity android:name=".admin.AdminActivity">
        </activity>
        <activity android:name=".registerLogin.RegiserActivity">
        </activity>
        <activity android:name=".registerLogin.LoginActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>